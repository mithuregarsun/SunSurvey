<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="suntechnologies.com.sunsurvey">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"

        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">

        </activity>
        <activity android:name=".AdminLogin"
            tools:ignore="DuplicateActivity">

        </activity>
        <activity android:name=".ForgotPassword"
            tools:ignore="DuplicateActivity">

        </activity>
        <activity android:name=".CreateSurvey"
            tools:ignore="DuplicateActivity">

        </activity>
        <activity android:name=".TakeSurveyUser"
            tools:ignore="DuplicateActivity">

        </activity>
        <activity android:name=".FeedBackUser"
            tools:ignore="DuplicateActivity">

        </activity>
        <activity android:name=".ShareQuestionUser"
            tools:ignore="DuplicateActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEND" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:mimeType="text/plain"/>
            </intent-filter>

        </activity>

    </application>

</manifest>