<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="suntechnologies.com.sunsurvey">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"

        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen"
            android:configChanges="orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>


        <activity android:name=".AdminLogin"
            android:windowSoftInputMode="stateAlwaysHidden"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            tools:ignore="DuplicateActivity"/>


        <activity android:name=".ForgotPassword"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            tools:ignore="DuplicateActivity"/>


        <activity android:name=".CreateSurvey"
            android:windowSoftInputMode="stateAlwaysHidden"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            tools:ignore="DuplicateActivity"/>


        <activity android:name=".TakeSurveyUser"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            tools:ignore="DuplicateActivity"/>


        <activity android:name=".FeedBackUser"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            tools:ignore="DuplicateActivity"/>


        <activity android:name=".SurveyDashBoard"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            tools:ignore="DuplicateActivity"/>


        <activity android:name=".TotalSurvey"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            tools:ignore="DuplicateActivity"/>



        <activity android:name=".SurveyResponse"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            tools:ignore="DuplicateActivity"/>



        <activity android:name="suntechnologies.com.sunsurvey.UserResponseSurvey"

            android:screenOrientation="portrait" />



        <activity android:name=".ShareQuestionUser"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            tools:ignore="DuplicateActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEND" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:mimeType="text/plain"/>
            </intent-filter>

        </activity>

    </application>

</manifest>